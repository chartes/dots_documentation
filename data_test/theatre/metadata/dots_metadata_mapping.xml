<?xml version="1.0" encoding="UTF-8"?>
<metadataMap xmlns="https://github.com/chartes/dots/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:html="http://www.w3.org/1999/xhtml">
  <mapping>
    <dc:title  scope="collection" format="tsv" source="./default_collections_titles.tsv" resourceId="collection_id" value="title"/>
    <dc:title scope="document" xpath="/TEI/teiHeader/fileDesc/titleStmt/title"/>
    <dc:creator scope="document" xpath="/TEI/teiHeader/fileDesc/titleStmt/author/@key"/>
    <creator_wikidata_url scope="document" xpath="/TEI/teiHeader/fileDesc/titleStmt/author/@ref"/>
    <dc:date scope="document" xpath="/TEI/teiHeader/profileDesc/creation/date/@when" type="number"/>
    <dct:license scope="document" xpath="/TEI/teiHeader/fileDesc/publicationStmt/availability/licence/@target"/>
  </mapping>
</metadataMap>