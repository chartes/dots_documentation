    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  },
  "servers": [
    {
      "url": "https://dev.chartes.psl.eu/dots/api/dts"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "Endpoint Basics",
        "operationId": "entryPoint",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/ld+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "context": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Resource not found"
          }
        }
      }
    },
    "/collections": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "Finds metadata resources",
        "description": "endpoint used for navigating collections",
        "operationId": "findResources",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "nav",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "children",
                "parents"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/ld+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "404": {
            "description": "Resource not found"
          }
        }
      }
    },
    "/navigation": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "Finds metadata resources",
        "description": "endpoint used for navigating collections",
        "operationId": "listPassages",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ref",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "start",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "end",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "down",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/ld+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Invalid status value"
          }
        }
      }
    },
    "/document": {
      "get": {
        "tags": [
          "default"
        ],
        "summary": "Finds documents",
        "operationId": "findDocument",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "description": "resource identifier",
            "required": true,
            "explode": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ref",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "start",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "end",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "format",
            "in": "query",
            "required": false,
            "content": {
              "text/html": {
                "schema": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/xml": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid tag value"
          }
        }
      }
    }
  }
